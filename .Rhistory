myFunction <- function () {
control <- rnorm(5, mean=0, sd=1)
treated <- rnorm(5, mean=0, sd=1)
Value <- c(control, treated)
Label <- c(rep("control", 5), rep("treated", 5))
t.test(Value ~ factor(Label))$p.value
}
myFunction()
}
}
for(i in 1:1000){
print(myFunction())
}
